<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- WARNING: Eclipse auto-generated file.
              Any modifications will be overwritten.
              To include a user specific buildfile here, simply create one in the same
              directory with the processing instruction <?eclipse.ant.import?>
              as the first entry and export the buildfile again. -->
<project basedir="." default="build" name="custom.ant.tasks">
	<property name="debuglevel" value="source,lines,vars" />
	<property name="target" value="1.5" />
	<property name="source" value="1.5" />
	<target name="init">
		<mkdir dir="tmp/eclipse" />
	</target>
	<target name="clean">
		<delete dir="tmp/eclipse" />
	</target>
	<target depends="init" name="build">
		<echo message="${ant.project.name}: ${ant.file}" />
		<javac debug="true" debuglevel="${debuglevel}" destdir="${custom.tasks.dir}/tmp/eclipse" source="${source}" target="${target}">
			<src path="${custom.tasks.dir}/src" />
			<classpath>
				<fileset dir="${eclipse.home}/plugins">
					<include name="org.apache.ant*/lib/*.jar" />
				</fileset>
			</classpath>
		</javac>
	</target>
</project>
