<attribute-sets>
  <attribute-set id="blogs">
    <value-set>
      <label>Blogs</label>
      <attribute key="OpenSearchDocumentSource.feedUrlTemplate" type="java.lang.String">
        <value>http://blogs.icerocket.com/search?q=${searchTerms}&amp;rss=1&amp;os=1&amp;p=${startPage}&amp;n=${count}&amp;tab=blog</value>
      </attribute>
      <attribute key="OpenSearchDocumentSource.resultsPerPage" type="java.lang.Integer">
        <value>50</value>
      </attribute>
      <attribute key="OpenSearchDocumentSource.maximumResults" type="java.lang.Integer">
        <value>200</value>
      </attribute>
    </value-set>
  </attribute-set>
</attribute-sets>