<attribute-sets>
  <attribute-set id="blogs">
    <value-set>
      <label>Blogs</label>
      <entry key="org.carrot2.source.opensearch.OpenSearchDocumentSource.feedUrlTemplate">
        <value type="java.lang.String" value="http://blogs.icerocket.com/search?q=${searchTerms}&amp;rss=1&amp;os=1&amp;p=${startPage}&amp;n=${count}&amp;tab=blog"/>
      </entry>
      <entry key="org.carrot2.source.opensearch.OpenSearchDocumentSource.resultsPerPage">
        <value type="java.lang.Integer" value="50"/>
      </entry>
      <entry key="org.carrot2.source.opensearch.OpenSearchDocumentSource.maximumResults">
        <value type="java.lang.Integer" value="200"/>
      </entry>
    </value-set>
  </attribute-set>
</attribute-sets>