<?xml version="1.0" encoding="UTF-8" ?>
<local-process id="yahoo-lingo">
  <name>YahooAPI-Lingo</name>
  <description>Search Yahoo, cluster using Lingo</description>

  <input  component-key="input-cached-yahooapi" />
  <filter component-key="filter-rawdocument-enumerator" />
  <filter component-key="filter-lingo" />
