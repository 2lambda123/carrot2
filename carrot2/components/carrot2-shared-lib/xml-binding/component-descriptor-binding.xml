<?xml version="1.0"?>

<!-- Castor binding file -->

<cbf:binding xmlns:cbf="http://www.castor.org/SourceGenerator/Binding"
             defaultBindingType='element'>
     
     <!--define the {namespace,package} binding -->

     <cbf:package>
          <cbf:name>com.dawidweiss.carrot.controller.carrot2.xmlbinding.componentDescriptor</cbf:name>
          <cbf:namespace>http://www.dawidweiss.com/projects/carrot/componentDescriptor</cbf:namespace>
     </cbf:package>

     <!-- Specific bindings -->
     
     <cbf:elementBinding name="service">
     	<cbf:java-class name="Service" final="false" equals="false" abstract="false" bound="false">
     	</cbf:java-class>
     </cbf:elementBinding>
     
     <cbf:elementBinding name="component">
        <cbf:java-class name="ComponentDescriptor" final="false" equals="false" abstract="false" bound="true">
        </cbf:java-class>
     </cbf:elementBinding>

    
 </cbf:binding>
     