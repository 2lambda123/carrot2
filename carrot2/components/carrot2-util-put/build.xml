
<project name="Carrot2 PUT Utilities" default="build">
	<description>
	This component will be removed in the future.
	</description>

    <property name="project.dir"      location="${basedir}" />
    <property name="carrot2.cvs.dir"  location="../.." />
    <property name="dist.name"        value="carrot2-util-put" />

    <property name="type"       	  value="jartype" />
    <import file="${carrot2.cvs.dir}/components/common-targets.inc" />

    <target name="javadoc">
        <property name="javadoc.packages" value="org.*" />
        <ant target="javadoc" antfile="${carrot2.cvs.dir}/components/master.xml" inheritall="true" inheritrefs="true" />
    </target>

</project>
