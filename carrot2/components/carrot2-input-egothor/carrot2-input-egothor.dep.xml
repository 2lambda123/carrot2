<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE component SYSTEM "../../etc/component-dependency.dtd" >

<component
    name="carrot2-input-egothor"
    description="Carrot2 Egothor Adapter">

    <provides profile="remote">
        <files build-path-exclude="true" prefix="tmp/dist">
            <file location="carrot2-input-egothor-remote.jar" />
        </files>
        <build>
            <ant file="build.remote.xml" target="build" />
        </build>
    </provides>

	<!-- Components -->
    <dependency name="carrot2-license" />
    <dependency name="carrot2-util-common" />

    <dependency name="xml-apis" profile="remote" />
    <dependency name="servlets-api" profile="remote" nocopy="true" />

</component>

