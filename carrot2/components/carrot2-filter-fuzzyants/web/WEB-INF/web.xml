<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Fuzzy Ants Clustering Filter</display-name>
  

<servlet>
    <servlet-name>FuzzyAntComponent</servlet-name>
    <servlet-class>com.dawidweiss.carrot.filter.FilterRequestProcessorServlet</servlet-class>
    <init-param>
      <param-name>filterRequestProcessor</param-name>
      <param-value>fuzzyAnts.FuzzyAnts</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>


<servlet-mapping>
    <servlet-name>FuzzyAntComponent</servlet-name>
    <url-pattern>/service/FuzzyAnts</url-pattern>
  </servlet-mapping>

</web-app>
