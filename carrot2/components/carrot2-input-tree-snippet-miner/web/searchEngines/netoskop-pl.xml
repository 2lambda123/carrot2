<?xml version="1.0" encoding="UTF-8"?>
<service>
  
    
  <request encoding="WINDOWS-1250">
    
        
    <service url="http://netoskop.pl/search/search" method="get" />
    

	
    <parameters>
      
            
      <parameter name="qt" mapto="query.string" />
      
            
      <parameter name="st" mapto="query.startFrom" />
      
            
      <parameter name="nm" value="50" />
      
        
    </parameters>
    

        
        
    <http-overrides>
      
            
      <header name="User-Agent">Mozilla/4.0 (compatible; MSIE 6.0; Windows NT 5.1)</header>
      
        
    </http-overrides>
    
    
  </request>
  

    
  <response encoding="WINDOWS-1250">
    
        
    <pageinfo>
      
            
      <expected-results-per-page>50</expected-results-per-page>
      
        
    </pageinfo>
    
    
    <extractor>
      <snippet>
        <dl>
          <dt>
            <a class="ab" />
          </dt>
          <dd description="beginOn">
            <br description="endBefore" />
            <a url="attribute:href" />
            <br />
            <img />
            <img />
          </dd>
        </dl>
      </snippet>
    </extractor>
  </response>
  

</service>

