<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>STC</display-name>
  <description>Implementation of STC algorithm.</description>
  <servlet>
    <servlet-name>Full STC</servlet-name>
    <servlet-class>com.dawidweiss.carrot.filter.FilterRequestProcessorServlet</servlet-class>
    <init-param>
      <param-name>log4j.properties</param-name>
      <param-value>/WEB-INF/log4j.properties</param-value>
    </init-param>
    <init-param>
      <param-name>filterRequestProcessor</param-name>
      <param-value>com.dawidweiss.carrot.filter.stc.servlet.STCFullServlet</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Full STC</servlet-name>
    <url-pattern>/service/fullstc</url-pattern>
  </servlet-mapping>
</web-app>
